name: Read File Content

on:
  push:
    branches:
      - main   # Trigger only when pushing to main branch

jobs:
  show-file-content:
    runs-on: ubuntu-latest

    steps:
      - name: Checkout repository
        uses: actions/checkout@v3

      - name: Display file content
        run: |
          echo "Reading file content..."
          cat example.txt
